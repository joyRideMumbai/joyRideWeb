if vendor.services.length > 0
	.mui-row.memberships.mui--z1
		.mui-col-md-12
			h4
				if type != "healthyfood"
					span.fa.fa-users
					span  Memberships
				else
					span.fa.fa-spoon
					span  Packages
			p.subtext 
				strong Why buy through Fitternity?
				span  Get insider discounts, avail easy payment options (Net-banking, Credit / Debit Card, EMI facility and Mobile wallet) and enjoy the lowest price guarantee.
			br
			.ratecard(data-accordion-group)
				each service, index in vendor.services
					.accordion(data-accordion)
						.heading(data-control data-card-title="#{service.name}")
							span.index #{index+1} 
							span.title #{service.name}
						.content(data-content)
							if type == "healthyfood"
								.healthyfood-service-description
									.mui-row
										.mui-col-md-8
											p !{service.short_description}
										.mui-col-md-4
											.gallery
												.photostrip
													if service.photos != undefined && service.photos.length > 0
														each photo,index in service.photos
															if index < 2
																a(href="https://b.fitn.in/s/g/full/#{photo.url}", title="#{photo.caption}", rel='nofollow')
																	.gallery-thumb(style="background-image:url(https://b.fitn.in/s/g/thumbs/#{photo.url})")
															if index == 3
																a(href="https://b.fitn.in/s/g/full/#{photo.url}", title="#{photo.caption}", rel='nofollow')
																	.gallery-thumb(style="background-image:url(https://b.fitn.in/s/g/thumbs/#{photo.url})")
																		span #{vendor.photos.length - 3}+
							each ratecard, rateindex in service.serviceratecard
								.mui-row.rate-item
									.mui-col-md-10.mui-col-xs-8
										.mui-row
											.mui-col-md-8.mui-col-xs-12.dur
												if ratecard.remarks != "" && ratecard.remarks != undefined
													if vendor.category_id != 45
														if ratecard.type == 'trial'
															//- if ratecard.duration > 0
															//- 	if ratecard.duration == 1
															//- 		if ratecard.duration_type == "sessions"
															//- 			span.duration(id="offer", itemprop="name") #{ratecard.duration} Trial session	
															//- 	else	
															//- 		span.duration(id="offer", itemprop="name") #{ratecard.duration} Trial #{ratecard.duration_type} 
															| Trial
														else
															if ratecard.validity > 0
																if ratecard.validity == 1
																	case ratecard.validity_type
																		when "days"
																			span.duration #{ratecard.validity} day 
																		when "months"
																			span.duration #{ratecard.validity} month 
																		when "years"
																			span.duration #{ratecard.validity} year 
																		default
																			span.duration #{ratecard.validity} #{ratecard.validity_type}
																else		
																	span.duration #{ratecard.validity} #{ratecard.validity_type} 
															if ratecard.duration > 0
																if ratecard.duration == 1
																	if ratecard.duration_type == "sessions"
																		span.duration(id="offer", itemprop="name") #{ratecard.duration} session	
																else	
																	span.duration(id="offer", itemprop="name") #{ratecard.duration} #{ratecard.duration_type} 
														meta(itemprop="priceCurrency",content="INR",id="offer")
														br
														small
															i (#{ratecard.remarks})
													else
														span.duration(id="offer", itemprop="name") !{ratecard.remarks}
												else
													if ratecard.type == 'trial'
															//- if ratecard.duration > 0
															//- 	if ratecard.duration == 1
															//- 		if ratecard.duration_type == "sessions"
															//- 			span.duration(id="offer", itemprop="name") #{ratecard.duration} Trial session	
															//- 	else	
															//- 		span.duration(id="offer", itemprop="name") #{ratecard.duration} Trial #{ratecard.duration_type} 
															| Trial session
													else
														if ratecard.validity > 0
															if ratecard.validity == 1
																case ratecard.validity_type
																	when "days"
																		span.duration #{ratecard.validity} day 
																	when "months"
																		span.duration #{ratecard.validity} month 
																	when "years"
																		span.duration #{ratecard.validity} year 
																	default
																		span.duration #{ratecard.validity} #{ratecard.validity_type} 
															else		
																span.duration #{ratecard.validity} #{ratecard.validity_type} 
														if ratecard.duration > 0
															if ratecard.duration == 1
																if ratecard.duration_type == "sessions"
																	span.duration(id="offer", itemprop="name") #{ratecard.duration} session	
															else	
																span.duration(id="offer", itemprop="name") #{ratecard.duration} #{ratecard.duration_type}
											.mui-col-md-4.mui-col-xs-12.pri
												if ratecard.special_price != ""
													.crossthis
														span.fa.fa-rupee 
														span.originalprice #{ratecard.price}
													span(itemprop="price",id="offer") #{ratecard.special_price}
													meta(itemprop="price", content ="#{ratecard.special_price}")
												else
													if ratecard.price > 0
														span(itemprop="price",id="offer")
															span.fa.fa-rupee 
															span #{ratecard.price}
													else
														| Free via Fitternity
									.mui-col-md-2.mui-col-xs-4.buy.mui--text-right
										if vendor.commercial_type != 0
											if ratecard.type == 'trial'
												if type != "healthyfood"
													a.mui-btn.mui-btn--small.mui-btn--primary(href="/booktrial/#{vendor.slug}") Book
											else if ratecard.type == 'workout session'
												a.mui-btn.mui-btn--small.mui-btn--primary(href="/workoutsession/#{vendor.slug}") Book
											else	
												a.mui-btn.mui-btn--small.mui-btn--primary(href="/buy/#{vendor.slug}/#{service._id}/#{ratecard._id}") buy
				br
				if vendor.commercial_type == 0
					.mui-row
						.mui-col-md-12
							.mui--text-subhead Note:
							p This rates are indicative and are subject to change. Please call the outlet directly to find our latest rates.