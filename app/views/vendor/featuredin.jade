if vendor.findercollections.length > 0
	.featuredin
		h4
			span.fa.fa-certificate
			span  Featured in
		br
		each collection in vendor.findercollections
			.mui-row.featured-item
				.mui-col-md-12
					a(href="/selections/#{vendor.city.slug}/#{collection.slug}" onclick="kyu1.pushevent('vendorfeatured',{vendor : #{vendor.slug} , value : #{collection.name}}'})").upper #{collection.name}
					br
					span.fa.fa-clone
					span.type.uppercase   selection
